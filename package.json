    "jest": {
        "rootDir": "scripts",
        "scriptPreprocessor": "../node_modules/6to5-jest",
        "testFileExtensions": [
            "es6",
            "js"
        ],
        "moduleFileExtensions": [
            "js",
            "json",
            "es6"
        ],
        "unmockedModulePathPatterns": [
