{
    "name": "griddle-react",
    "version": "0.2.20",
    "description": "Custom version of griddle-react for nsi purposes",
    "keywords": [
        "react-component",
        "grid",
        "react",
        "pagination",
        "sort"
    ],
    "main": "modules/griddle.js",
    "dependencies": {
        "babel": "^5.1.13",
        "babel-runtime": "^5.1.13",
        "object-assign": "^2.0.0",
        "pre-commit": "^1.1.3",
        "underscore": "^1.8.3",
        "versiony": "^1.4.0"
    },
    "peerDependencies": {
        "react": "^0.13.2"
    },
    "devDependencies": {
        "6to5-jest": "*",
        "babel": "^5.6.14",
        "babel-core": "^5.6.15",
        "react-chartist": "~0.5.0",
        "react-tools": "~0.13.x",
        "underscore": "~1.8.3",
        "webpack": "~1.8.10",
        "webpack-dev-server": "~1.8.2"
    },
    "scripts": {
        "test": "jest",
        "make": "make clean build",
        "prepublish": "make clean build",
        "version": "node update-version.js && git add ."
